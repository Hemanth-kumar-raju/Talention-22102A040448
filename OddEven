public class Main
{
	public static void main(String[] args) {
		int[] a={10,3,2,6,8,5,30,21};
		int odd=0;
		int even=0;
		for(int n:a){
		    if(n%2==0){
		        even++;
		    }else{
		        odd++;
		    }
		}
		System.out.println("count of Even:"+even);
		System.out.println("count of Odd:"+odd);
	}
}

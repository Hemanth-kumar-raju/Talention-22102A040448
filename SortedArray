public class Main
{
	public static void main(String[] args) {
		int[] arr={1,2,9,5,4,7,8};
		boolean sorted=true;
		for(int i=1;i<arr.length;i++){
		    if(arr[i]<arr[i-1]){
		        sorted=false;
		        break;
		    }
		}
	    if(sorted){
	        System.out.println("It is sorted ");
		    }else{
		        System.out.println("It is not sorted");
	    }
	}
}

public class Main {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4, 5};

        int i= 0;              
        int j= arr.length-1; 

        if (arr.length>=2) {
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }
        for (int n:arr) {
            System.out.print(n+ " ");
        }
    }
}
